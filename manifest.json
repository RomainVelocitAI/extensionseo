{
  "manifest_version": 3,
  "host_permissions": ["<all_urls>"],
  "name": "SEO QuickScore",
  "version": "1.0",
  "description": "Analyze and score the SEO of the current webpage",
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "icon.png"
  },
  "permissions": ["activeTab", "clipboardWrite", "scripting"],
  "icons": {
    "48": "icon.png"
  }
}
